{
  "name": "exile-diary",
  "version": "0.1.1",
  "main": "main.js",
  "build": {
    "appId": "com.pathofexile.exilediary",
    "productName": "Exile Diary",
    "win": {
      "target": "nsis",
      "icon": "./res/icons/win/ExileDiary.ico"
    },
    "nsis": {
      "oneClick": "false",
      "allowElevation": "true",
      "allowToChangeInstallationDirectory": "true",
      "installerIcon": "./res/icons/win/ExileDiary.ico",
      "uninstallerIcon": "./res/icons/win/ExileDiary.ico",
      "installerHeaderIcon": "./res/icons/win/ExileDiary.ico"
    }
  },
  "scripts": {
    "start": "electron .",
    "postinstall": "install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "util",
    "functional",
    "server",
    "client",
    "browser"
  ],
  "author": "joshi",
  "contributors": [],
  "dependencies": {
    "color-convert": "^1.9.3",
    "jimp": "^0.4.0",
    "js-levenshtein": "^1.1.3",
    "merge-img": "^2.1.2",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "nodejs-tail": "^1.0.2",
    "request": "^2.0.0",
    "sqlite3": "^4.0.2",
    "tesseract.js": "^1.0.10",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-builder": "^20.28.3"
  }
}
